{
  "name": "lemmy-bot",
  "version": "0.0.1",
  "contributors": [
    {
      "name": "helkarakse",
      "email": "noreply@helkarakse.com"
    }
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "Proprietary",
  "private": true,
  "scripts": {
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "build": "tsc",
    "start": "npm run build && node ./dist/index.js",
    "dev": "npx nodemon -e ts -w src/ --exec 'npm run start'",
    "watch": "npx nodemon -e ts -w src/ --exec 'npm run build'",
    "test": "jest",
    "migrate-up": "npm run build && node dist/Classes/Database/Umzug.js up",
    "migrate-down": "npm run build && node dist/Classes/Database/Umzug.js down"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.6",
    "@types/pg": "^8.10.2",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-file-extension-in-import-ts": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "jest-extended": "^4.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "umzug": "^3.3.1"
  },
  "dependencies": {
    "common-tags": "^1.8.2",
    "cron-parser": "^4.8.1",
    "dotenv": "^16.3.1",
    "inversify": "^6.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "lemmy-bot": "^0.4.1",
    "lemmy-js-client": "^0.18.1-rc.3",
    "moment": "^2.29.4",
    "mysql2": "^3.6.0",
    "pg": "8.2.x",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5"
  },
  "repository": "git@github.com:monyet-cc/automonyet.git"
}
